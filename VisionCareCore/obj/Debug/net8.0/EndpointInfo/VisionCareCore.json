{
  "openapi": "3.0.1",
  "info": {
    "title": "VisionCareCore API",
    "description": "VisionCareCore API",
    "termsOfService": "http://localhost:5000/swagger/index.html",
    "contact": {
      "name": "Bruno",
      "email": "brunomoisespalomino@gmail.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "v1"
  },
  "paths": {
    "/amsac/v1/authentication/sign-in": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SignInResource"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/SignInResource"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/SignInResource"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/amsac/v1/authentication/sign-up": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SignUpResource"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/SignUpResource"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/SignUpResource"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/amsac/v1/authentication/refresh-token": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/amsac/v1/authentication/sign-out": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/amsac/v1/auth-user/{authUserId}": {
      "get": {
        "tags": [
          "AuthUser"
        ],
        "parameters": [
          {
            "name": "authUserId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/amsac/v1/auth-user": {
      "get": {
        "tags": [
          "AuthUser"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/amsac/v1/auth-user/me": {
      "get": {
        "tags": [
          "AuthUser"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/vc/v1/gpt/test": {
      "post": {
        "tags": [
          "Gpt"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GptRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/GptRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/GptRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "GptRequest": {
        "type": "object",
        "properties": {
          "tipo": {
            "type": "string",
            "nullable": true
          },
          "texto": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SignInResource": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SignUpResource": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "lastName": {
            "type": "string",
            "nullable": true
          },
          "registerArea": {
            "type": "string",
            "nullable": true
          },
          "dateCreated": {
            "type": "string",
            "format": "date-time"
          },
          "role": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "Ingrese el token JWT con el prefijo 'Bearer '",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}